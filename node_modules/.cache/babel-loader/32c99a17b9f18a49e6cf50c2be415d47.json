                 </div>\n \n                         <div class=\"radio-box d-flex\">\n                             <input type=\"radio\" name=\"genero\" value=\"Other\" id=\"other\" v-model=\"user.gender\" /> \n                             <label for=\"other\">Other</label>\n                         </div>\n \n                     </div>\n \n \n                     <input type=\"button\" value=\"Submit\" @click=\"showInfo()\" class=\"btn-submit  btn-form\" />\n \n                </form>\n                   \n        </div>\n        \n        <div class=\"info-form\" v-if=\"show\">\n            <h2>Form Info</h2>\n            <div class=\"info-user\" v-html=\"agroupInfoForm\"></div>\n        </div>\n         \n</div>\n      \n\n</template>\n\n<style>\n  .form-vue{\n    width: 40%;\n    margin-bottom: 50px;\n  }\n  .form-flex{\n      display: flex;\n      justify-content: space-around;\n  }\n  .info-user{\n      text-align: start;\n  }\n  \n</style>\n<script>\nimport {required,mingLength} from \"vuelidate/lib/validators\";\n\nexport default {\n    name: 'Form',\n    components:{\n    },\n    validations:{\n        name: {\n            required,\n            mingLength: mingLength(2)\n        }, \n        surname: {\n            required,\n            mingLength: mingLength(2)\n        },\n        biography: {\n            required,\n            mingLength: mingLength(10)\n        },\n    },\n    computed:{\n        agroupInfoForm(){\n            const info= \"<strong> Name: \"+this.user.name+\"</strong> <br/>\"+\n                        \"<strong> Last Name: \"+this.user.surname+\"</strong> <br/>\"+\n                        \"<strong> Gender: \"+this.user.gender+\"</strong> <br/>\"+\n                        \"<hr> Biography:  \"+this.user.biography;\n\n\n            return info;\n                      \n\n        }\n    },\n    methods:{\n        showInfo(){\n            console.log(this.user);\n            if(this.user.name != null && this.user.surname && this.user.biography && this.user.gender){\n                this.show = true;\n            }\n        }\n    },\n    data(){\n        return{\n            user:{\n                name: '',\n                surname: '',\n                biography: '',\n                gender: ''\n            },\n            show : false\n        }\n    }\n}\n</script>"]}]}